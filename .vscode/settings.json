{
  "deno.enablePaths": [
    "supabase/functions"
  ],
  "deno.lint": true,
  "deno.unstable": [
    "bare-node-builtins",
    "byonm",
    "sloppy-imports",
    "unsafe-proto",
    "webgpu",
    "broadcast-channel",
    "worker-options",
    "cron",
    "kv",
    "ffi",
    "fs",
    "http",
    "net"
  ],
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[sql]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": null
  },
  "[plpgsql]": {
    "editor.formatOnSave": false
  },
  "cSpell.words": [
    "supabase"
  ]
}